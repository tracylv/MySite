<div class="errorpage context">
    <div class="ro whitebg">
        <div class="ce w12">
            <h2>是不是在做坏事？出错了: <%= statusCode %> <%= statusText %></h2>
            <% if(geddy.config.environment == 'development'){ %>
                <% if (stack) { %>
                <code><%= stack %></code>
                <% } else if (typeof message != 'undefined') { %>
                <code><%= message %></code>
                <% } %>
            <% } %>
        </div>
    </div>
</div>



